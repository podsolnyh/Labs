#include "stdafx.h"
#include <iostream>
using namespace std;
int main()
{
	int quantity, max = 0,days;
	for (int i = 0; i < 30; i++)
	{

		cout << "Enter the amount of precipitation in " << i+1 << " day:";
		cin >> quantity;
	 if(quantity>=max){
		max = quantity;
		days = i + 1;
		}

	}
	cout <<"Day with the most precipitation:"<< days<<endl;
		return 0;
}
