//59
#include <iostream>
#include <string>
#include <Windows.h>
using namespace std;
int main() {
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	string s;
	getline(cin, s);
	bool f = 1;
	for (int i = 0; i < s.size() - 1; ++i) if (s[i] == 'ш' && s[i + 1] == 'ы')f = 0; else if (s[i] == 'ж' && s[i + 1] == 'ы') f = 0;
	cout << ((f == 1) ? "True" : "False");
}
